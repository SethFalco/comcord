<configuration xmlns="https://commandler.elypia.org"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="https://commandler.elypia.org application.xsd">
  <commandler>
    <config>org.elypia.comcord.DiscordConfig</config>
    <integration>org.elypia.comcord.DiscordIntegration</integration>
    <adapter type="org.elypia.comcord.adapters.EmoteAdapter">
      <compatible>net.dv8tion.jda.api.entities.Emote</compatible>
    </adapter>
    <adapter type="org.elypia.comcord.adapters.GuildAdapter">
      <compatible>net.dv8tion.jda.api.entities.Guild</compatible>
    </adapter>
    <adapter type="org.elypia.comcord.adapters.RoleAdapter">
      <compatible>net.dv8tion.jda.api.entities.Role</compatible>
    </adapter>
    <adapter type="org.elypia.comcord.adapters.TextChannelAdapter">
      <compatible>net.dv8tion.jda.api.entities.TextChannel</compatible>
    </adapter>
    <adapter type="org.elypia.comcord.adapters.UserAdapter">
      <compatible>net.dv8tion.jda.api.entities.User</compatible>
    </adapter>
    <adapter type="org.elypia.comcord.adapters.VoiceChannelAdapter">
      <compatible>net.dv8tion.jda.api.entities.VoiceChannel</compatible>
    </adapter>
    <messenger type="org.elypia.comcord.messengers.EmbedBuilderMessenger" provides="net.dv8tion.jda.api.entities.Message">
      <compatible>net.dv8tion.jda.api.EmbedBuilder</compatible>
    </messenger>
    <messenger type="org.elypia.comcord.messengers.MessageEmbedMessenger" provides="net.dv8tion.jda.api.entities.Message">
      <compatible>net.dv8tion.jda.api.entities.MessageEmbed</compatible>
    </messenger>
    <messenger type="org.elypia.comcord.messengers.MessageMessenger" provides="net.dv8tion.jda.api.entities.Message">
      <compatible>net.dv8tion.jda.api.entities.Message</compatible>
      <compatible>net.dv8tion.jda.internal.entities.DataMessage</compatible>
    </messenger>
    <messenger type="org.elypia.comcord.messengers.MiscToMessageMessenger" provides="net.dv8tion.jda.api.entities.Message">
      <compatible>java.lang.Boolean</compatible>
      <compatible>java.lang.CharSequence</compatible>
      <compatible>java.lang.Character</compatible>
      <compatible>java.net.URL</compatible>
    </messenger>
    <messenger type="org.elypia.comcord.messengers.NumberToMessageMessenger" provides="net.dv8tion.jda.api.entities.Message">
      <compatible>java.lang.Number</compatible>
      <compatible>java.lang.Double</compatible>
      <compatible>java.lang.Float</compatible>
      <compatible>java.lang.Long</compatible>
      <compatible>java.lang.Integer</compatible>
      <compatible>java.lang.Short</compatible>
      <compatible>java.lang.Byte</compatible>
    </messenger>
  </commandler>
</configuration>
